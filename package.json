{
  "name": "AgileFrance2017",
  "version": "0.0.1",
  "description": "This is sources for Agile France 2017 website, built with Gulp",
  "config": {
    "target_url": "2017.conf.agile-france.org"
  },
  "scripts": {
    "build": "gulp build",
